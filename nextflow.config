conda.enabled = true
// conda.useMamba
nextflow.enable.moduleBinaries = true

dag.overwrite = true

conda.cacheDir = ".env"


process {
    penv     = 'smp'
    executor = 'sge'

    withLabel: use_8_cpus {
            cpus   = 2
            memory = 4.GB
            errorStrategy = 'retry'
       }
}
