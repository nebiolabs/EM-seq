conda.enabled = true
process.penv = 'smp'
nextflow.enable.moduleBinaries = true
// conda.useMamba = true

process {
    
    executor='sge'

    withLabel: cpus_8 {
        cpus   = 4
        memory = 15.GB
        errorStrategy = 'retry'
    }

    withLabel: cpus_2 {
        cpus   = 2
        memory = 15.GB
        errorStrategy = 'retry'
    }
}
