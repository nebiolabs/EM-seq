conda.enabled = true
nextflow.enable.moduleBinaries = true


process {
    
    // executor='sge'

    withLabel: cpus_8 {
        cpus   = 4
        memory = 15.GB
        errorStrategy = 'retry'
    }

    withLabel: cpus_2 {
        cpus   = 2
        memory = 15.GB
        errorStrategy = 'retry'
    }
}
